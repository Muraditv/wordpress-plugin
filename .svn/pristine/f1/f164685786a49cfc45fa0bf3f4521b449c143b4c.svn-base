#toplevel_page_s3bubble_oembed .wp-menu-name {
	font-size: 0.8em;
    margin-left: 45px;
}
#toplevel_page_s3bubble_oembed .wp-menu-image {
	background-image: url(../img/s3bubblelogo.png);
	background-size: cover;
}
.s3bubble-video-wrapper {
	position: relative;
	padding-bottom: 56.25%; /* 16:9 */
	padding-top: 25px;
	height: 0;
}
.s3bubble-video-wrapper iframe {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
}
.chosen-container {
	width: 100% !important;
}